.main_con {
    margin-left: 260px;
    margin-right: 30px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
}

.account_content {
    width: 100%;
    padding: 20px;
}

.account_search {
    height: 90px;
    padding-left: 20px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
}

.account_search .account_search_t {
    font-size: 14px;
    color: #333;
    font-weight: bold;
    margin: 0;
    padding-top: 10px;
}

.search_con {
    padding-top: 6px;
}

.word_c {
    font-size: 14px;
    color: #666;
    margin: 0 10px
}

.key_word {
    font-size: 14px;
    color: #666;
    display: inline-block;
    padding-right: 20px;
    padding-top: 10px;
}

.account_search input {
    display: inline-block;
    width: 144px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    border: 1px solid #ddd;
    text-indent: 1em;
    border-radius: 3px;
}

.search_btn {
    display: inline-block;
    width: 50px;
    height: 28px;
    line-height: 28px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    background-color: #7b7de4;
    border-radius: 4px;
    margin-left: 10px;
    cursor: pointer;
}

#keywords {
    margin-right: 60px;
}

.account_table {
    width: 100%;
    font-size: 14px;
    border: 1px solid #ddd;
}

.account_table thead tr {
    width: 100%;
    height: 50px;
    font-size: 14px;
    color: #333;
    background-color: #f0edfc;
}

.account_table th, .account_table td {
    width: 11%;
    text-align: center;
}

.account_table th:first-child, .account_table td:first-child {
    width: 6%;
}

.account_table th:last-child, .account_table td:last-child {
    width: 17%;
    padding-right: 10px;
}

.account_table td {
    height: 20px;
    line-height: 20px;
    padding: 10px 0px;
    color: #666;
}

/*.account_table td:last-child {*/
    /*color: #7b7de4;*/
    /*cursor: pointer;*/
/*}*/

.account_foot {
    width: 100%;
    position: relative;
    padding-top: 20px;
    padding-bottom: 60px;
}

.account_total {
    position: absolute;
    left: 50%;
    padding-right: 60px;
}

