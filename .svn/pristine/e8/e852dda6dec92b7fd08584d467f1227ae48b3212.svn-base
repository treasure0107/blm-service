.main_con {
    margin-left: 260px;
    margin-right: 30px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
}

.news_content {
    width: 100%;
    padding: 20px;
}

.news_search {
    height: 90px;
    padding-left: 20px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
}

.news_search_t {
    font-size: 14px;
    color: #333;
    font-weight: bold;
    margin: 0;
    padding-top: 10px;
}

.key_word {
    font-size: 14px;
    color: #666;
    display: inline-block;
    padding-right: 20px;
    padding-top: 10px;
}

.news_search input {
    display: inline-block;
    width: 144px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    border: 1px solid #ddd;
    text-indent: 1em;
    border-radius: 3px;
}

.search_btn {
    display: inline-block;
    width: 50px;
    height: 28px;
    line-height: 28px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    background-color: #7b7de4;
    border-radius: 4px;
    margin-left: 10px;
    cursor: pointer;
}

#keywords {
    margin-right: 30px;
}

.news_table {
    width: 100%;
    font-size: 14px;
    border: 1px solid #ddd;
}

.news_table thead tr {
    width: 100%;
    height: 60px;
    font-size: 14px;
    color: #333;
    background-color: #f0edfc;
}

.news_table th, .news_table td {
    width: 20%;
    text-align: center;
}

.news_table th:first-child, .news_table td:first-child {
    width: 5%;
}

.news_table th:last-child, .news_table td:last-child {
    width: 15%;
    padding: 0 10px;
}

.news_table .news_tit{
    width: 25%;
}

.news_table .news_con{
    width: 35%;
}
.news_table td:last-child{
    color:#7b7de4;
    cursor: pointer;
}

.news_table td {
    height: 20px;
    line-height: 20px;
    padding: 20px 0px;
    color: #666;
}

.news_table td:last-child a {
    color: #7b7de4;
    cursor: pointer;
}
.news_table tr:hover{
    background-color: #fef4e8;
}
.news_foot {
    width: 100%;
    position: relative;
    padding-top: 20px;
    padding-bottom: 40px;
}


/*分页*/

.page_icon {
    font-size: 14px;
    position: absolute;
    top: 26px;
    right: 69px;
    color: #888;
}

.page_icon span, .prevPage, .lastPage {
    display: inline-block;
    width: 28px;
    height: 28px;
    line-height: 28px;
    text-align: center;
    border: 1px solid #ddd;
    margin-right: 10px;
}

.page_icon a {
    color: #888;
}