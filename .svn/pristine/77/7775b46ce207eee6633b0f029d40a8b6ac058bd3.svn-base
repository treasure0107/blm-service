:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #999;
    opacity: 1;
}

::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #999;
    opacity: 1;
}

input:-ms-input-placeholder, textarea::-webkit-input-placeholder {
    color: #999;
    opacity: 1;
}

input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
    color: #999;
    opacity: 1;
}

.main_con {
    margin-left: 260px;
    margin-right: 30px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
}

.main_con ul {
    width: 100%;
    height: 80px;
    /*line-height: 60px;*/
    box-shadow: 3px 0px 16px rgba(197, 197, 199, .5);
}

.main_con .contract_tit li {
    width: 100px;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    color: #333;
    margin-left: 30px;
    margin-top: 25px;
    float: left;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
    border-radius: 15px;
    text-align: center;
    cursor: pointer;
}

.titActive {
    background-color: #7b7de4;
    color: #fff !important;
}

.main_content {
    padding: 0px 20px 20px;
}

.contract_search {
    height: 56px;
    line-height: 56px;
    font-size: 14px;
}

.contract_search .contract_search_t {
    display: inline-block;
    color: #333;
    font-weight: bold;
    padding-left: 20px;
    padding-right: 58px;
}

.contract_search .key_word {
    display: inline-block;
    color: #666;
    padding-right: 10px;
}

.contract_search input {
    display: inline-block;
    width: 144px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    border: 1px solid #ddd;
    text-indent: 1em;
    border-radius: 3px;
}

.search_btn {
    display: inline-block;
    width: 50px;
    height: 28px;
    line-height: 28px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    background-color: #7b7de4;
    border-radius: 4px;
    margin-left: 10px;
}

.contract_table {
    width: 100%;
    font-size: 14px;
    border: 1px solid #ddd;
}

.contract_table thead tr {
    width: 100%;
    height: 60px;
    font-size:14px;
    color:#333;
    background-color: #f0edfc;
}

.contract_table th, .contract_table td {
    width: 10%;
    text-align: center;
}

.contract_table th:first-child, .contract_table td:first-child {
    width: 6%;
}

.contract_table th:last-child, .contract_table td:last-child {
    width: 14%;
}

.contract_table td {
    height: 50px;
    line-height: 50px;
    color:#666;
}

.contract_table td:last-child {
    color: #7b7de4;
    cursor: pointer;
}

.page_con{
    width: 100%;
    /*height: 100px;*/
    padding-top: 20px;
    padding-bottom: 50px;
    position: relative;
}
.page_icon{
    font-size: 14px;
    position: absolute;
    right: 69px;
    color:#888;
}
.page_icon span,.prevPage,.lastPage{
    display: inline-block;
    width: 28px;
    height: 28px;
    line-height: 28px;
    text-align: center;
    border:1px solid #ddd;
    margin-right: 10px;
}
.page_icon a{
    color:#888;
}
a:hover, a:visited, a:link, a:active {
    text-decoration: none;
    color:#888;
}

