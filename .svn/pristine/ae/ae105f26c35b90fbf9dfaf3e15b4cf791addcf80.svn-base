.account_popup_main .remark_main {
    height: 80px;
    width: 100%;
    font-size: 14px;
    color: #333;
    line-height: 19px;
    padding: 20px 109px;
    background-color: #fff;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
    overflow-y: auto;
    position: relative;
    z-index: 9;
    display: none;
}

.propOffImg {
    padding: 6px;
    position: absolute;
    top: 10px;
    right: 40px;
    /*z-index: 999;*/
}

