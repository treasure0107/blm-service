.main_con {
    margin-left: 260px;
    margin-right: 30px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
    padding-bottom: 20px;
}

h2 {
    padding: 0;
    margin: 0;
}

select {
    /*Chrome和Firefox里面的边框是不一样的，所以复写了一下*/
    border: solid 1px #ddd;
    /*很关键：将默认的select选择框样式清除*/
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    /*为下拉小箭头留出一点位置，避免被文字覆盖*/
    padding-right: 6px;
    margin-left: 6px;
    outline: none;
    text-indent: 0.3em;
}

.account_content {
    width: 100%;
    padding: 20px;
    /*display: none;*/
}

.account_search {
    height: 90px;
    padding-left: 20px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
}

.account_search .account_search_t {
    font-size: 14px;
    color: #333;
    font-weight: bold;
    margin: 0;
    padding-top: 10px;
}

.search_con {
    padding-top: 6px;
}

.word_c {
    font-size: 14px;
    color: #666;
    margin: 0 10px
}

.key_word {
    font-size: 14px;
    color: #666;
    display: inline-block;
    padding-right: 20px;
    padding-top: 10px;
}

.account_search input {
    display: inline-block;
    width: 144px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    border: 1px solid #ddd;
    text-indent: 1em;
    border-radius: 3px;
}

.account_search select {
    display: inline-block;
    width: 136px;
    height: 30px;
    line-height: 30px;
    font-size: 12px;
    border: 1px solid #ddd;
    text-indent: 0.5em;
    border-radius: 3px;
}

.search_btn {
    display: inline-block;
    width: 50px;
    height: 28px;
    line-height: 28px;
    font-size: 14px;
    color: #fff;
    text-align: center;
    background-color: #7b7de4;
    border-radius: 4px;
    margin-left: 10px;
    cursor: pointer;
}

#keywords {
    margin-right: 60px;
    background: url("../images/iocn_arrow.png") no-repeat scroll 116px center transparent;
    color: #666;
}

.account_table {
    width: 100%;
    font-size: 14px;
    border: 1px solid #ddd;
}

.account_table thead tr {
    width: 100%;
    height: 50px;
    font-size: 14px;
    color: #333;
    background-color: #f0edfc;
}

.account_table th, .account_table td {
    width: 14%;
    text-align: center;
}

.account_table th:first-child, .account_table td:first-child {
    width: 6%;
}

.account_table th:last-child, .account_table td:last-child {
    width: 10%;
    padding: 0 10px;
}

.account_table td {
    height: 20px;
    line-height: 20px;
    padding: 20px 0;
    color: #666;
}

.account_table td:last-child a {
    color: #7b7de4;
    cursor: pointer;
}

.account_foot {
    width: 100%;
    position: relative;
    padding-top: 20px;
    padding-bottom: 40px;
}

/*弹窗*/
.account_popup {
    /*padding: 20px;*/
    margin: 20px 50px;
    padding-bottom: 30px;
    box-shadow: 2px 0px 16px rgba(197, 197, 199, .5);
    color: #333;
    /*background-color: #f7f6f6;*/
    position: relative;
    display: none;
}

.account_popup h2 {
    width: 100%;
    font-size: 16px;
    font-weight: bold;
    padding-top: 20px;
    text-align: center;
}

.account_popup .unit_y {
    width: 100%;
    font-size: 12px;
    text-align: center;
    padding-top: 10px;
    padding-bottom: 10px;
}

.account_popup_main {
    margin: 0 60px;
    /*box-shadow: 2px 0 16px rgba(197, 197, 199, .5);*/
    /*border-bottom: 1px solid #ddd;*/
}

.account_popup_main li {
    width: 100%;
    height: 60px;
    background-color: #fff;
    /*margin-top: 3px;*/
    border-radius: 3px;
    overflow: hidden;
    border-bottom: 1px solid #f1f1f1;
}

.account_popup_main li .account_popup_tit {
    float: left;
    width: 104px;
    line-height: 60px;
    /*background-color: #e5deff;*/
    /*text-align: center;*/
    font-size: 16px;
}

.account_popup_main li p {
    margin: 0;
}

.account_popup_main .account_popup_c {
    float: left;
    width: 104px;
    margin-top: 10px;
    text-align: center;
    margin-left: 20px;
}

.account_popup_c .account_c {
    padding-bottom: 6px;
}

.account_popup_main .disposable_account {
    height: 121px;
}

.account_popup_main .disposable_account .account_popup_tit {
    height: 121px;
    line-height: 121px;
}

.disposable_account_o {
    overflow: hidden;
}

.disposable_account_b {
    display: block;
    height: 1px;
    margin: 3px 0 0 126px;
    background-color: #f1f1f1;
}

.account_popup_c p:last-child {
    color: #777;
}

