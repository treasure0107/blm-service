/*弹窗*/
.pop_up {
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    font-size: 16px;
    color: #fff;
    width: 199px;
    text-align: center;
    height: 52px;
    line-height: 52px;
    background-color: rgba(0, 0, 0, .7);
    border-radius: 23px;
    -webkit-border-radius: 23px;
    -moz-border-radius: 23px;
    -ms-border-radius: 23px;
    cursor: pointer;
    display: none;
    z-index: 199999999999999;
}